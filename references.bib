@InProceedings{Rohlig:2015,
  Title                    = {Supporting {Activity} {Recognition} by {Visual} {Analytics}},
  Author                   = {Röhlig, Martin and Luboschik, Martin and Krüger, Frank and Kirste, Thomas and Schumann, Heidrun and Bögl, Markus and Alsallakh, Bilal and Miksch, Silvia},
  Booktitle                = {{IEEE} {Conference} on {Visual} {Analytics} {Science} and {Technology} ({VAST})},
  Year                     = {2015},

  Address                  = {Chicago, Illinois, USA},
  Month                    = oct,
  Pages                    = {41--48},
  Publisher                = {IEEE},

  Abstract                 = {Recognizing activities has become increasingly relevant in many application domains, such as security or ambient assisted living. To handle different scenarios, the underlying automated algorithms are configured using multiple input parameters. However, the in- fluence and interplay of these parameters is often not clear, making exhaustive evaluations necessary. On this account, we propose a visual analytics approach to supporting users in understanding the complex relationships among parameters, recognized activities, and associated accuracies. First, representative parameter settings are determined. Then, the respective output is computed and statisti- cally analyzed to assess parameters' influence in general. Finally, visualizing the parameter settings along with the activities provides overview and allows to investigate the computed results in detail. Coordinated interaction helps to explore dependencies, compare different settings, and examine individual activities. By integrating automated, visual, and interactive means users can select parameter values that meet desired quality criteria. We demonstrate the application of our solution in a use case with realistic complexity, involving a study of human protagonists in daily living with respect to hundreds of parameter settings.},
  Date-modified            = {2017-08-21 12:29:18 +0000},
  File                     = {07347629.pdf:/Users/fk169/Library/Application Support/Zotero/Profiles/laozb6lc.default/zotero/storage/T6B37DA3/07347629.pdf:application/pdf;Roehlig15_ActivityRecognitionVisualAnalytics.pdf:/Users/fk169/Library/Application Support/Zotero/Profiles/laozb6lc.default/zotero/storage/QH577UIH/Roehlig15_ActivityRecognitionVisualAnalytics.pdf:application/pdf;segvis_draft_20150326.pdf:/Users/fk169/Library/Application Support/Zotero/Profiles/laozb6lc.default/zotero/storage/VTF88B24/segvis_draft_20150326.pdf:application/pdf}
}

