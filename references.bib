@InProceedings{Loseries:2016,
  Title                    = {Von der {Lernumgebung} zum forschungsbasierten {Lehr}-{Lern}-{Prozess}},
  Author                   = {Loseries, Wiebke and Reinholz, Heidi and Kirste, Thomas and Krüger, Frank and Schennigcke, Bastian and Sucharowski, Wolfgang and von Oeynhausen, Viola},
  Booktitle                = {{GDCP}-{Jahrestagung}},
  Year                     = {2016},

  Address                  = {Zürich},
  Month                    = sep,

  Abstract                 = {Forschendes Lernen setzt sich zunehmend als Lehr-Lern-Konzept in der naturwissenschaftlichen Bildung des Sachunterrichts durch. Dabei stellt die Vermittlung physikalischer Inhalte eine gro{\ss}e Herausforderung dar. Im Rahmen des Lehr-Lern-Labors PhySch - Physik und Schule werden Lehr-Lern-Einheiten gestaltet, so dass sie einen forschenden Charakter haben, dabei Raum für kreatives Spiel lassen und in ihrer Gesamtheit eine soziale Struktur bilden, in der Lernende wie Lehrende an diesem Prozess teilnehmen. Gemeinsam mit Lehramtsstudierenden wurde hierzu eine Lernumgebung für SchülerInnen zum Thema Zeit entwickelt. Aus der erfolgreichen Umsetzung dieser Lernumgebung ist ein innovativer Forschungsprozess zur Untersuchung von Interaktionsprozessen hervorgegangen. In Form einer Laborstudie wurde dieses Forschungsprojekt als Interventionsstudie im Schuljahr 2015/16 in einem computergestützten Labor pilotiert. Die stete Bewegung zwischen Praxis und Forschung hin zu einem forschungsbasierten Lehr-Lern-Prozess und dessen Auslösern sowie Wirkzusammenhängen werden im Fokus der Präsentation stehen.},
  Date-modified            = {2017-08-21 12:29:18 +0000}
}

